{
   "background": {
      "service_worker": "service_worker.js"
   },
   "description": "a very flexible website blocker",
   "icons": {
      "48": "images/dog.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "flexible blocker",
   "options_page": "options.html",
